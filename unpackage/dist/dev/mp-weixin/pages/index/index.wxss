.view-index {
  font-size: 14px;
  background-color: #efefef;
}
.view-index .view-nav {
  background: #d7ab7e;
  height: 44px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #ffffff;
}
.view-index .view-nav .nav-left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.view-index .view-nav .nav-left .place-title {
  line-height: 1;
}
.view-index .view-nav .nav-left .place-title .place-title-name {
  font-size: 17px;
  font-weight: bold;
  padding: 0px 0px 3px 12px;
}
.view-index .view-nav .nav-left .place-title .place-title-explain {
  font-size: 12px;
  -webkit-transform: scale(0.8);
          transform: scale(0.8);
}
.view-index .view-nav .nav-left .nav-logo {
  width: 31px;
  height: 31px;
  padding: 0px 0px 0px 10px;
}
.view-index .view-nav .nav-left .nav-logo .logo-img {
  width: 100%;
  height: 100%;
}
.view-index .view-nav .nav-right {
  padding: 0px 22px 0px 0px;
  font-size: 20px;
}


.tui-banner-box {
	width: 100%;
	/* padding: 0 30rpx; */
	box-sizing: border-box;
	/* position: absolute;
 */
	/* overflow: hidden; */
	z-index: 99;
	bottom: -80rpx;
	left: 0;
}
.tui-slide-image {
	width: 100%;
	height: 100%;
	display: block;
	/* padding: 16rpx 30rpx 0px 30rpx; */
}
.tui-banner-swiper {
	width: 100%;
	height: 100%;
	/* border-radius: 12rpx; */
	overflow: hidden;
	-webkit-transform: translateY(0);
	        transform: translateY(0);
}
.tui-banner-swiper .wx-swiper-dot {
	width: 8rpx;
	height: 8rpx;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: inline-flex;
	background: none;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tui-banner-swiper .wx-swiper-dot::before {
	content: '';
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	background-color: rgba(255, 255, 255, 0.8);
	border-radius: 16rpx;
	overflow: hidden;
}
.tui-banner-swiper .wx-swiper-dot-active::before {
	background-color: #fff;
}
.tui-banner-swiper .wx-swiper-dot.wx-swiper-dot-active {
	width: 16rpx;
}





































