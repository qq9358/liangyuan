		}
	}],
	"easycom": {
		"autoscan": true,
		"custom": {
			"tui-(.*)": "@/components/thorui/tui-$1/tui-$1.vue",
			"custom-(.*)": "@/components/custom/custom-$1/custom-$1.vue",
			"ticket-(.*)": "@/components/ticket/ticket-$1/ticket-$1.vue",
			"uni-(.*)": "@/components/uni/uni-$1/uni-$1.vue"
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "不一样的星球",
		"navigationBarBackgroundColor": "#ffffff",
		"backgroundColor": "#FFFFFF"
	},
	"tabBar": {
	    "color": "#7A7E83",
	    "selectedColor": "#099fde",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [{
	        "pagePath": "pages/index/index",
	        "iconPath": "static/images/tabbar/home.png",
	        "selectedIconPath": "static/images/tabbar/home-blue.png",
	        "text": "主页"
	    }, {
	        "pagePath": "pages/index/order",
	        "iconPath": "static/images/tabbar/cate.png",
	        "selectedIconPath": "static/images/tabbar/cate-blue.png",
	        "text": "订单"
	    }, {
	        "pagePath": "pages/index/member-center",
	        "iconPath": "static/images/tabbar/user.png",
	        "selectedIconPath": "static/images/tabbar/user-blue.png",
	        "text": "我"
	    }]
	},
	"subPackages": [
		{
			"root": "pagesMember",
			"pages": [
				{
					"path": "my-integral",
					"style": {
						"navigationBarTitleText": "我的积分",
						"navigationBarBackgroundColor":"#19a0f0",
						"navigationBarTextStyle": "white"
					}
				},{
					"path": "bind-member",
					"style": {
						"navigationBarTitleText": "会员绑定"
					}
