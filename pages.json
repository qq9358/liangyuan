{
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"app-plus": {
				"titleNView": false
			}
		}
	}, {
		"path": "pages/index/order",
		"style": {
			"navigationBarTitleText": "我的订单"
		}
	}, {
